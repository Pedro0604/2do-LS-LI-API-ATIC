@page "/titulares/vehiculos/{Id:int?}";
@inject NavigationManager Navigator;

<ListaTitulares/>
<Dialogo CerrarDialogo="Cerrar">
  <ListaVehiculos IdTitular="@Id"/>
</Dialogo>

@code {
    [Parameter] public int Id { get; set; }
    void Cerrar()
    { 
      Navigator.NavigateTo("/titulares");
    }
}