@page "/terceros/ingresar/{Id:int?}";
@inject NavigationManager Navigator;

<ListaTerceros/>
<Dialogo CerrarDialogo="Cerrar">
  <IngresarTercero CerrarDialogo="Cerrar" Id="@Id"/>
</Dialogo>

@code {
    [Parameter] public int Id { get; set; }
    void Cerrar()
    { 
      Navigator.NavigateTo("/terceros");
    }
}