@page "/vehiculos/ingresar/{Id:int?}";
@inject NavigationManager Navigator;

<ListaVehiculos/>
<Dialogo CerrarDialogo="Cerrar">
  <IngresarVehiculo CerrarDialogo="Cerrar" Id="@Id"/>
</Dialogo>
<VolverAtras @ref="volver"/>

@code {
    [Parameter] public int Id { get; set; }
    VolverAtras volver = null!;
    void Cerrar()
    { 
      volver.Cerrar();
    }
}