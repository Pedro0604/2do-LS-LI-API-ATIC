@page "/siniestros/ingresar/{Id:int?}";
@inject NavigationManager Navigator;

<ListaSiniestros/>
<Dialogo CerrarDialogo="Cerrar">
  <IngresarSiniestro CerrarDialogo="Cerrar" Id="@Id"/>
</Dialogo>

@code {
    [Parameter] public int Id { get; set; }
    void Cerrar()
    { 
      Navigator.NavigateTo("/siniestros");
    }
}